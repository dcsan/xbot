{
  "name": "cofbot",
  "version": "0.0.2",
  "description": "cofbot fun",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "build/module/index.js",
  "repository": "https://github.com/dcsan/cofbot",
  "license": "MIT",
  "keywords": [],
  "scripts": {
    "build": "run-s clean build:main",
    "build:main": "tsc -p tsconfig.json",
    "clean": "trash build test",
    "tsdev": "LOG_LEVEL=debug nodemon src/index.ts",
    "dev": "NODE_ENV=development LOG_LEVEL=debug run-s build:main mon",
    "test": "run-s build test:unit",
    "test:unit": "jest --coverage",
    "describe": "npm-scripts-info",
    "start:node": "NODE_ENV=development node dist/index.js",
    "test:watch": "run-s clean build:main && run-p \"build:main -- -w\" \"test:unit -- --watch\"",
    "start:dev": "run-s clean build:main && run-p \"build:main -- -w\" \"mon\"",
    "mon": "nodemon dist/index.js",
    "xdev": "ts-node src/index.ts",
    "tdev": "ts-node-dev ./src/index.ts",
    "build:all": "run-s clean && run-p build:*",
    "build:module": "tsc -p tsconfig.module.json",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:tslint": "tslint --fix --project .",
    "test:lint": "tslint --project . && prettier \"src/**/*.ts\" --list-different",
    "cov": "run-s build test:unit cov:html && open-cli coverage/index.html",
    "cov:html": "nyc report --reporter=html",
    "cov:send": "nyc report --reporter=lcov && codecov",
    "cov:check": "nyc report && nyc check-coverage --lines 100 --functions 100 --branches 100",
    "doc": "run-s doc:html && open-cli build/docs/index.html",
    "doc:html": "typedoc src/ --exclude node_modules --target ES6 --mode file --out build/docs",
    "doc:json": "typedoc src/ --exclude **/*.test.ts --target ES6 --mode file --json build/docs/typedoc.json",
    "doc:publish": "gh-pages -m \"[ci skip] Updates\" -d build/docs",
    "version": "standard-version",
    "reset": "git clean -dfx && git reset --hard && npm i",
    "prepare-release": "run-s reset test cov:check doc:html version doc:publish"
  },
  "scripts-info": {
    "info": "Display information about the package scripts",
    "build": "Clean and rebuild the project",
    "fix": "Try to automatically fix any linting problems",
    "test": "Lint and unit test the project",
    "watch": "Watch and rebuild the project on save, then rerun relevant tests",
    "cov": "Rebuild, run tests, then create and open the coverage report",
    "doc": "Generate HTML API documentation and open it in a browser",
    "doc:json": "Generate API documentation in typedoc JSON format",
    "version": "Bump package.json version, update CHANGELOG.md, tag release",
    "reset": "Delete all untracked files and reset the repo to the last commit",
    "prepare-release": "One-step: clean, build, test, publish docs, and prep a release"
  },
  "engines": {
    "node": ">=8.9"
  },
  "dependencies": {
    "botbuilder-adapter-slack": "^1.0.11",
    "botbuilder-storage-mongodb": "^0.9.5",
    "botkit": "^4.9.0",
    "botkit-plugin-cms": "^1.0.3",
    "dotenv": "^7.0.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@bitjson/npm-scripts-info": "^1.0.0",
    "@bitjson/typedoc": "^0.15.0-0",
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/jest": "*",
    "@types/node": "*",
    "codecov": "^3.5.0",
    "cz-conventional-changelog": "^2.1.0",
    "gh-pages": "^2.0.1",
    "jest": "^26.1.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "open-cli": "^5.0.0",
    "prettier": "^1.18.2",
    "standard-version": "^6.0.1",
    "trash-cli": "^3.0.0",
    "ts-jest": "*",
    "ts-node-dev": "^1.0.0-pre.54",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-immutable": "^6.0.1",
    "typescript": "^3.9.7"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "prettier": {
    "singleQuote": true,
    "semi": false
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "exclude": [
      "**/*.spec.js"
    ]
  }
}